		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@tsconfig/svelte": "^5.0.4",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.9.3",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.64",
		"clsx": "^2.1.1",
